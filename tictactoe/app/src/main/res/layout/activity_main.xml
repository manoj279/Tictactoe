<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/b"
    tools:context="com.example.jagadish.tictactoe.MainActivity">

    <ImageButton
        android:layout_width="150px"
        android:layout_height="175px"
        android:id="@+id/imageButton0"
        android:src="@drawable/c"
        android:layout_alignParentTop="true"
        android:layout_marginTop="140dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <ImageButton
        android:layout_width="175px"
        android:layout_height="175px"
        android:id="@+id/imageButton1"
        android:src="@drawable/d"
        android:layout_alignTop="@+id/imageButton0"
        android:layout_toRightOf="@+id/imageButton0"
        android:layout_toEndOf="@+id/imageButton0"
        android:layout_marginLeft="33dp"
        android:layout_marginStart="33dp" />
    <ImageButton
        android:layout_width="205px"
        android:layout_height="175px"
        android:id="@+id/imageButton2"
        android:src="@drawable/e"
        android:layout_alignTop="@+id/imageButton1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <ImageButton
        android:layout_width="150px"
        android:layout_height="255px"
        android:id="@+id/imageButton10"
        android:src="@drawable/f"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/imageButton0"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <ImageButton
        android:layout_width="150px"
        android:layout_height="300px"
        android:id="@+id/imageButton20"
        android:src="@drawable/g"
        android:layout_marginTop="48dp"
        android:layout_below="@+id/imageButton10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <ImageButton
        android:layout_width="175px"
        android:layout_height="390px"
        android:id="@+id/imageButton11"
        android:src="@drawable/h"
        android:layout_alignLeft="@+id/imageButton1"
        android:layout_alignStart="@+id/imageButton1"
        android:layout_above="@+id/imageButton20"
        android:layout_alignTop="@+id/imageButton10" />
    <ImageButton
        android:layout_width="175px"
        android:layout_height="250px"
        android:id="@+id/imageButton21"
        android:src="@drawable/i"
        android:layout_alignTop="@+id/imageButton20"
        android:layout_alignLeft="@+id/imageButton11"
        android:layout_alignStart="@+id/imageButton11"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/imageButton11"
        android:layout_alignEnd="@+id/imageButton11" />
    <ImageButton
        android:layout_width="275px"
        android:layout_height="245px"
        android:id="@+id/imageButton12"
        android:src="@drawable/j"
        android:layout_alignTop="@+id/imageButton11"
        android:layout_alignLeft="@+id/imageButton2"
        android:layout_alignStart="@+id/imageButton2" />
    <ImageButton
        android:layout_width="275px"
        android:layout_height="325px"
        android:id="@+id/imageButton22"
        android:src="@drawable/k"
        android:layout_alignTop="@+id/imageButton21"
        android:layout_alignLeft="@+id/imageButton12"
        android:layout_alignStart="@+id/imageButton12" />

</RelativeLayout>